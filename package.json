{
  "name": "karozu",
  "version": "0.0.8",
  "description": "Handlebars alternative with end-to-end typesafety",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./utils": "./dist/utils/index.js"
  },
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils/index.d.ts"
      ]
    }
  },
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w"
  },
  "keywords": [
    "typescript",
    "handlebars",
    "template",
    "type-safety"
  ],
  "author": "Nico Albanese",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.11.25",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
